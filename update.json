{
  "type": "Scaffold",
  "appBar": {
    "type": "AppBar",
    "title": {
      "type": "Text",
      "data": "Hey, Dynamic"
    }
  },
  "body": {
    "type": "Center",
    "child": {
      "type": "Column",
      "mainAxisAlignment": "MainAxisAlignment.center",  // MainAxisAlignment.center (enum index 1)
      "children": [
        {
          "type": "Text",
          "data": "You have pushed the button this many times:"
        },
        {
          "type": "Text",
          "data": "\$_counter",  // Placeholder for the counter value
          "style": {
            "fontSize": 24
          }
        }
      ]
    }
  },
  "floatingActionButton": {
    "type": "FloatingActionButton",
    "tooltip": "Increment",
    "onPressed": "_incrementCounter",
    "child": {
      "type": "Icon",
      "icon": "Icons.add"
    }
  }
}
